{
  "manifest_version": 2,

  "name": "Life Count Down",
  "description": "This extension reminds you that your life is limited, do whatever you mostly wanted to do",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
